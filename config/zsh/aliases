# General aliases
alias vim=nvim
alias p=pnpm
alias b=bun
alias y=yarn
alias e=expo
alias g=git
alias v=vim
alias vv=nvim .
alias cat=bat
alias c3=c3c
killport() {
    if [ -z "$1" ]; then
        echo "Usage: killport <port>"
        return 1
    fi
    local pid=$(lsof -ti:$1)
    if [ -z "$pid" ]; then
        echo "No process found on port $1"
        return 1
    fi
    echo "Killing process $pid on port $1"
    kill $pid
}
alias installapk='adb install -r'
